struct Lazy[A]:
    x :: Var[Mb[A]] = Var(Null)
    init(f :: ()->A)
    nameCall
     :: ()->A##this and the next lines are comments
     :: A #
      = fold $
        _ => do:
            (a = f()).toJust().setTo(x)
            pure $ a
        id, x
         